	<parameter name="id_evaluacion" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT

navegador,evaluacion.fecha,versionNavegador,descripcion,heuristico.nombre as heuristicoNombre,criterio,puntos,
evaluador.nombre as nombreEvaluador,evaluador.apellido,evaluador.email,cargo,profesion,telefono,empresaEvaluador,urlSitio,nombreSitio,tipoSitio,proyecto.fecha



FROM evaluacion, heuristico, subheuristico, resultado, calificacion, evaluador, detalle, proyecto
WHERE heuristico.id_heuristico = subheuristico.id_heuristico
AND subheuristico.id_subheuristico = calificacion.id_subheuristico
AND resultado.id_resultado = calificacion.id_resultado
AND evaluacion.id_evaluacion = resultado.id_evaluacion
AND evaluador.id_evaluador = detalle.id_evaluador
AND proyecto.id_proyecto = detalle.id_proyecto
AND proyecto.id_proyecto = evaluacion.id_proyecto
AND evaluacion.id_evaluador = evaluador.id_evaluador
AND evaluacion.id_evaluacion =$P{id_evaluacion} ORDER BY heuristico.id_heuristico]]>
	</queryString>
